{
  "name": "OMGX_contracts-analyzer",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "TARGET=ovm hardhat compile --show-stack-traces",
    "analyze": "hardhat run scripts/analyze.js",
    "lint": "eslint --ignore-path .gitignore",
    "postinstall": "patch-package",
    "deploy:local": "hardhat --network local run scripts/deployLocal.js",
    "deploy:rinkeby": "hardhat --network rinkeby run scripts/deployRinkeby.js",
    "test:local": "hardhat test"
  },
  "dependencies": {
    "@eth-optimism/contracts": "^0.2.7",
    "@eth-optimism/hardhat-ovm": "^0.0.3",
    "@eth-optimism/plugins": "^0.0.16",
    "@ethersproject/providers": "^5.1.2",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^3.1.0",
    "@tenderly/hardhat-tenderly": "^1.0.12",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.1",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.1.4",
    "expect": "^26.6.2",
    "hardhat": "2.0.6",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@boringcrypto/boring-solidity": "boringcrypto/BoringSolidity#e06e943",
    "@openzeppelin/contracts": "^3.1.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.1.4"
  }
}
